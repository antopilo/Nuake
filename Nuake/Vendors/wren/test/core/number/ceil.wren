System.print(123.ceil)      // expect: 123
System.print((-123).ceil)   // expect: -123
System.print(0.ceil)        // expect: 0
System.print((-0).ceil)     // expect: -0
System.print(0.123.ceil)    // expect: 1
System.print(12.3.ceil)     // expect: 13
System.print((-0.123).ceil) // expect: -0
System.print((-12.3).ceil)  // expect: -12
