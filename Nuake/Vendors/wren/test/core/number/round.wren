System.print(123.round)      // expect: 123
System.print((-123).round)   // expect: -123
System.print(0.round)        // expect: 0
System.print((-0).round)     // expect: -0
System.print(0.123.round)    // expect: 0
System.print(12.3.round)     // expect: 12
System.print((-0.123).round) // expect: -0
System.print((-12.3).round)  // expect: -12
