System.print(123.floor)      // expect: 123
System.print((-123).floor)   // expect: -123
System.print(0.floor)        // expect: 0
System.print((-0).floor)     // expect: -0
System.print(0.123.floor)    // expect: 0
System.print(12.3.floor)     // expect: 12
System.print((-0.123).floor) // expect: -1
System.print((-12.3).floor)  // expect: -13
